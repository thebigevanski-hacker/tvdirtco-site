{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/dashboard/login",
      "destination": "/dashboard/login.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard/index.html"
    },
    {
      "source": "/login",
      "destination": "/dashboard/login.html"
    },
    {
      "source": "/booking",
      "destination": "/booking/step-1-equipment.html"
    },
    {
      "source": "/book",
      "destination": "/booking/step-1-equipment.html"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/services.html",
      "destination": "/services",
      "permanent": true
    },
    {
      "source": "/rentals.html",
      "destination": "/rentals",
      "permanent": true
    },
    {
      "source": "/about.html",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/projects.html",
      "destination": "/projects",
      "permanent": true
    },
    {
      "source": "/contact.html",
      "destination": "/contact",
      "permanent": true
    },
    {
      "source": "/dashboard/login.html",
      "destination": "/dashboard/login",
      "permanent": true
    },
    {
      "source": "/dashboard/index.html",
      "destination": "/dashboard",
      "permanent": true
    },
    {
      "source": "/booking/step-1-equipment.html",
      "destination": "/booking",
      "permanent": true
    },
    {
      "source": "/booking/step-2-dates.html",
      "destination": "/booking/step-2-dates",
      "permanent": true
    },
    {
      "source": "/booking/step-3-delivery.html",
      "destination": "/booking/step-3-delivery",
      "permanent": true
    },
    {
      "source": "/booking/step-4-contact.html",
      "destination": "/booking/step-4-contact",
      "permanent": true
    },
    {
      "source": "/booking/step-5-confirm.html",
      "destination": "/booking/step-5-confirm",
      "permanent": true
    },
    {
      "source": "/booking/success.html",
      "destination": "/booking/success",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
